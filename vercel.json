{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/(chat)/api/chat/route.ts": {
      "maxDuration": 60
    },
    "app/(chat)/api/jobs/stream/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "POSTGRES_URL": "@postgres_url",
    "AUTH_SECRET": "@auth_secret",
    "AI_GATEWAY_API_KEY": "@ai_gateway_api_key"
  }
}